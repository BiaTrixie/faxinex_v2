{
  "expo": {
    "name": "faxinex",
    "slug": "faxinex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-faxinex.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo-faxinex.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}