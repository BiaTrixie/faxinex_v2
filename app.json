{
  "expo": {
    "name": "faxinex",
    "slug": "faxinex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-faxinex.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.biatrixie.faxinex"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo-faxinex.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5f03396d-e270-4012-98ab-77298ea9558f"
      }
    }
  }
}
